{
    "package": "com.huawei.agconnect.demo",
    "name": "AGCDemo",
    "versionName": "1.0.0",
    "versionCode": 1,
    "icon": "/Common/logo.png",
    "minPlatformVersion": 1060,
    "features": [
        {
            "name": "system.router"
        },
        {
            "name": "system.prompt"
        },
        {
            "name": "service.account"
        },
        {
            "name": "system.fetch"
        },
        {
            "name": "system.storage"
        },
        {
            "name": "system.configuration"
        }
    ],
    "permissions": [
        {
            "origin": "*"
        }
    ],
    "config": {},
    "router": {
        "entry": "Main",
        "pages": {
            "Auth": {
                "component": "auth",
                "path": "/Auth"
            },
            "Main": {
                "component": "index",
                "path": "/Main"
            }
        }
    },
    "display": {
        "pages": {
        }
    }
}