# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'

target 'AGConnectAuthDemo' do
  
  use_frameworks!
  pod "AGConnectAuth"
  
  pod 'WechatOpenSDK'
  pod 'TencentOpenApiSDK'
  pod 'WeiboSDK'
  pod 'FBSDKLoginKit', "~> 6.0.0"
  pod 'GoogleSignIn',"~>5.0.2"
  pod 'TwitterKit',"~>3.4.2"
  
end

target 'AGConnectAuthDemo-Swift' do
  
  use_frameworks!
  pod "AGConnectAuth"
  
  pod 'WechatOpenSDK'
  pod 'TencentOpenApiSDK'
  pod 'WeiboSDK'
  pod 'FBSDKLoginKit', "~> 6.0.0"
  pod 'GoogleSignIn',"~>5.0.2"
  pod 'TwitterKit',"~>3.4.2"
  
end
