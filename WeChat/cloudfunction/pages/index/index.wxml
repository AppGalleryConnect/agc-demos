      <text>\n</text>
      <view class="weui-cell__bd" style="margin: 30rpx 0">
        <input class="weui-input" name="httpTrigger" placeholder="input your HttpTrigger" />
      </view>
      <view class="weui-cell__bd" style="margin: 30rpx 0">
        <input class="weui-input" name="body" placeholder="input your Body" />
